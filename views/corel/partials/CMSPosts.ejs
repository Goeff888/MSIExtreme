<h2>meine Posts</h2>
<div class="gallery">
 <ul>
  <li>Posts aus dem Content Management System</li>
 </ul>
</div>


<!-- Modal zum hinzufÃ¼gen eines neuen History-Eintrags-->
<div class="modal fade" id="historyModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Neues Bild</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <form method="post" action="/corel/<%= /*corel._id*/ %>/edit?_method=PUT" encType="multipart/form-data">
      <div class="modal-body">
        Beschreibung:
        <textarea rows="4" cols="50" name="description"></textarea>
        <input type="file" name="image">
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="submit" class="btn btn-primary">Save changes</button>
      </div>
      </form>
    </div>
  </div>
</div>